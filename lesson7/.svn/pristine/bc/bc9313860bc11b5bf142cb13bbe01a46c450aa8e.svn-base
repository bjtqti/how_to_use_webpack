@charset "utf-8";
/* CSS Document */

.polyCon{
	width: 100%;
	overflow-x: hidden;
}
.polyCon #page-content{
}
/* 类别 */
.polyTabs {
	height: 1.16rem;
	border-bottom: 1px solid #d7d7d7;
	background-color:#fff;
	position:relative;
	z-index:2;
}
.polyTabs ul {
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	width: 100%;
	height: 1.16rem;
}
.polyTabs ul li {
	-webkit-appearance: none;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	display: block;
	line-height: 1.16rem;
	text-align: center;
}
.polyTabs ul li.current {
	border-bottom:.053333rem solid #801da1;
}
.polyTabs ul li.current i{
	color: #801da1;
    
}
.polyCon{
	width: 100%;
	overflow-x: hidden;
}
.polyCon #page-content{
}
.polyCon #page-content > div{
	width:100%;
	position:absolute;
}

.orderSpeed{
}
.orderSpeed > span{
	display:block;
	text-align:center;
	color:#801da1;
	font-size:.27rem;
	padding-bottom:.32rem;
}
.orderSpeed .orderNum{
	padding:.32rem;
	border-bottom:1px solid #d7d7d7;
	color:#999;
	font-size:.38rem;
}
.orderSpeed .orderNum span{
	color:#2c2f35;
	font-weight:bold;
}
.orderSpeed .rate{
	padding:.32rem .32rem;
}
.orderSpeed .rate img{
	width:100%;
	padding:0 .68rem;
}
.orderSpeed .rateText{
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
}
.orderSpeed .rateText span{
	display: block;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	text-align:center;
	font-size:.27rem;
}
.orderSpeed .rateText span.notTo{
	color:#999;
}

.order-time {
    background: #fff;
    padding: .32rem;
    position: relative;
    border-top: .266667rem solid #f5f5f5;
    border-bottom: .266667rem solid #f5f5f5;
}
.order-time p {
    padding: 1% 0;
	font-size:.38rem;
}
.order-time p span.mobNum{
	font-weight:bold;
	margin-left:.64rem;
}
.order-time p.fs12px{
	font-size:.32rem;
}

.order-list {
	border-bottom: .266667rem solid #f5f5f5;
}
.order-info {
    border-bottom: 1px solid #d7d7d7;
    padding: .12rem .32rem;
}
.order-info span {
    display: block;
    font-size: .32rem;
    padding: .12rem 0;
}
.orderConfirm_l {
}
.J_moveRight {
    position: relative;
}
.J_moveRight > div {
    border-bottom: 1px solid #d7d7d7;
    padding: .16rem .32rem;
}
.J_moveRight > div:last-child{
	border-bottom:0;
}
.J_moveRight .img_wrap {
    width: 2rem;
    padding: .08rem;
    border: 1px solid #d7d7d7;
    background-color: #fff;
    float: left;
    font-size: 0;
    overflow: hidden;
    margin-top: .1rem;
    display: block;
}
.J_moveRight .img_wrap img {
    width: 100%;
}
.J_moveRight .gd_info {
    position: relative;
    width: 75%;
    padding-left: .266667rem;
    /* padding-bottom: .266667rem; */
    overflow: hidden;
}
.J_moveRight .gd_info .name {
    width: 100%;
    line-height: .5rem;
    height: 1rem;
    margin: .16rem 0 0;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    float: left;
}
.J_moveRight .gd_info .name span {
    font-size: .32rem;
    color: #333;
}
.J_moveRight .gd_info .value {
    width: 100%;
    height: .8rem;
    float: left;
    text-align: left;
    margin: .16rem 0 0 0;
}
.J_moveRight .gd_info .value span {
    font-size: .32rem;
    margin-right: .24rem;
}
.J_moveRight .gd_info .value b {
    font-size: .266667rem;
    font-weight: normal;
    line-height: 25px;
    color: #333;
}

.count-box {
    margin-top: 0;
    background-color: #fff;
    padding-bottom: 0;
    border-bottom: .266667rem solid #f5f5f5;
}
.count-box .title {
    height: 1rem;
    line-height: 1rem;
    border-bottom: 1px solid #f5f5f5;
    padding: 0 .32rem;
}
.count-box .bottom-line {
    height: .96rem;
    line-height: .96rem;
    position: relative;
    padding: 0 .32rem;
}
.count-box .bottom-line .label {
    float: left;
    color: #999;
}
.count-box .bottom-line .data {
    float: right;
    color: #333;
	font-size: .266667rem;
}
.count-box .bottom-line .data span {
    margin-left: .04rem;
	font-size: .373333rem;
}
.bottom-line.intro .data {
    color: #27c241;
}
.bottom-line.no-border .label {
    color: #333;
}
.count-box .bottom-line .data.red-w {
    color: #D32449;
}
.count-box .bottom-line .red-box {
	position: absolute;
	right: 85px;
	top: 5px;
	z-index: 1;
	float: right;
	width: .986667rem;
	height: .5333333rem;
	line-height: .5333333rem;
	background: url(../images/ico5.gif) right no-repeat;
	background-size: auto 100%;
	color: #D32449;
	font-size: .27rem;
	text-align: center;
	padding-right: .12rem;
}

/* btns */
.confirmBtns {
    border-top: 1px solid #d1d1d1;
    padding: .18rem .32rem;
    text-align: right;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background-color: #fff;
    z-index: 21;
	text-align:center;
}
.confirmBtns a.confirm_btn {
    display: inline-block;
    background-color: #dd1847;
    color: #fff;
    border-radius:20px;
    line-height: .94rem;
    border: 1px solid #dd1847;
    width: 40%;
    text-align: center;
	margin:0 2%;
}
.confirmBtns a.confirmBorder_btn{
	background-color:#fff;
	color:#dd1847;
}

/* 物流信息 */
.logistics{
}
.logistics .info{
	padding:0 .32rem .266667rem;
	border-bottom:.266667rem solid #f5f5f5;
}
.logistics .info span{
	display:block;
	border-bottom:1px solid #d7d7d7;
	padding:.266667rem 0;
	font-size:.32rem;
	margin-bottom:.266667rem;
}
.logistics .info p{
	font-size:.38rem;
	line-height:.76rem;
}
.logistics .rate{
	padding:0 .32rem;
}
.logistics .rate > span{
	display:block;
	border-bottom:1px solid #d7d7d7;
	padding:.266667rem 0;
	font-size:.32rem;
	margin-bottom:.266667rem;
}
.org-wuliuStatusList {
    width: 100%;
}
.org-wuliuStatusList .org-wuliuStatus .org-wuLiuLeft em {
    display: block;
    vertical-align: top;
    position: absolute;
}
.org-wuliuStatusList .org-wuliuStatus {
    width: 100%;
    font-size: 0.37333333rem;
    position: relative;
    padding-top: 0.50666667rem;
}
.org-wuliuStatusList .org-midBg {
    background: url(../images/org_line.png) -7px -15px no-repeat;
}
.org-wuliuStatusList .org-curBg {
    background: url(../images/org_line.png) -7px 12px no-repeat;
}
.org-wuliuStatusList .org-starBg {
    background: url(../images/org_line.png) -7px -195px no-repeat;
}
.org-wuliuStatusList .org-wuliuStatus.org-curBg .org-wuLiuLeft .org-middle {
    width:.25rem;
    height:.25rem;
    background: url(../images/org_wuLiuCur.png);
    background-size: 100%;
    top:.54rem;
    left:.01rem;
}
.org-wuliuStatusList .org-wuliuStatus .org-wuLiuLeft .org-middle {
    width:.25rem;
    height:.25rem;
    background: url(../images/org_wuLiuEver.png);
    background-size: 100%;
    top: .58rem;
    left:.01rem;
}
.org-wuliuStatusList .org-wuliuStatus .org-wuLiuFont {
    width: 100%;
    display: inline-block;
    padding-left: .6rem;
}
.org-wuliuStatusList .org-wuliuStatus .org-wuLiuFont span {
    display: inline-block;
	font-size:.32rem;
	color:#666;
}
.org-wuliuStatusList .org-wuliuStatus .org-wuLiuFont .org-wuLiuTime {
    width: 100%;
    padding: 0.37333333rem 0;
    border-bottom: 1px solid #e0e0e0;
	color:#333;
}
.org-wuliuStatusList .org-wuliuStatus.org-curBg .org-wuLiuFont span{
	color: #1ba11b;
}
.org-wuliuStatusList .org-wuliuStatus .org-wuLiuMid {
    color: #999999;
}
.logistics .rate > span.source{
	border-bottom:0;
	margin-left:.6rem;
}