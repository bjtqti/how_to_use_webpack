@charset "utf-8";
/* CSS Document */

@-webkit-keyframes fadeIn{
	0%{
		opacity:0
	}100%{
		opacity:1
	}
}
.fadeIn{
	-webkit-animation:fadeIn 1s;
}

body.grey{
	background-color:#f7f7f7;
}
.searchResult header{
	    background: #fff url(../images/headerBg_2.png) 0 bottom no-repeat;
}
header .screening {
    position: absolute;
    top: 0;
    right: 0;
    width: 1.15rem;
    height: 1.15rem;
    line-height: 1.15rem;
    text-align: right;
    text-align: center;
    color: #801da1;
}

.searchResult .search{
	background-color:#fff;
}
.search-wrap {
    width: 100%;
}
.search-header {
    height:1.16rem;
    background-color: #fff;
	border-bottom:1px solid #d7d7d7;
}
.search-box {
    position: absolute;
    left:.266667rem;
    top:.133333rem;
    right:1.6rem;
    overflow: hidden;
}
.searchResult .search-box {
	left:1.6rem;
	top:.16rem;
}
.search-btn {
    float:right;
    width:60px;
    text-align: center;
    line-height:1.16rem;
	color:#801da1;
}
.search-box input{
    background-color: #E3E3E6;
    border-radius:5px;
    height: .8rem;
    color: #333;
    font-size: 0.32rem;
    width: 100%;
    padding-left:.86rem;
    line-height:normal;
}
.search-box span{
    position: absolute;
    color: #666;
    width:17px;
    height:17px;
    text-align: center;
    top: .15rem;
    left: .2rem;
	background:url(../images/searchIcon.png) 0 0 no-repeat;
	background-size:100%;
}
.search-box.searchFoucs span{
	background:url(../images/searchIcon.png) 0 -25px no-repeat;
	background-size:100%;
}
.search-box .reset{
	position:absolute;
	left:auto;
	right:.18rem;
	top:.08rem;
	display:none;
	line-height:normal;
}
.search-box .reset .iconfont{
	font-size:.42rem;
}

.searchHot{
	border-bottom:1px solid #d7d7d7;
	padding:.16rem .32rem;
	background-color:#fdfdfd;
}
.searchHot span{
	display:inline-block;
	line-height:1rem;
}
.searchHot div{
	float:right;
	width:88%;
}
.searchHot a{
	padding:.14rem .18rem;
	display:inline-block;
	border-radius:5px;
	border:1px solid #999;
	margin:.16rem .13rem;
	line-height:.38rem;
	font-size:.32rem;
}
.searchList{
	padding:0 .32rem;
	background-color:#f7f7f7;
}
.searchList span{
	display:block;
	border-bottom:1px solid #d7d7d7;
	height:1.33334rem;
	margin-top:.4rem;
}
.searchList span em{
	float:left;
	display:block;
	font-size:.38rem;
	line-height:1.1rem;
}
.searchList span i{
	float:right;
	color:#801da1;
	border:1px solid #801da1;
	display:block;
	border-radius:3px;
	padding:.12rem .16rem;
	line-height:.6rem;
	font-size:.38rem;
}
.searchList a{
	display:block;
	border-bottom:1px solid #d7d7d7;
	height:1.2rem;
	line-height:1.2rem;
	width:100%;
	overflow:hidden;
	font-size:.32rem;
}

.searchOut{
	padding:0 .32rem;
	background-color:#f7f7f7;
	display:none;
}
.searchOut a{
	display:block;
	border-bottom:1px solid #d7d7d7;
	height:1.2rem;
	line-height:1.2rem;
	width:100%;
	overflow:hidden;
	font-size:.32rem;
}

/* 搜索结果页 */
header .screening {
    position: absolute;
    top: 0;
    right: 0;
    width: 1.15rem;
    height: 1.15rem;
	line-height: 1.15rem;
    text-align: right;
	text-align:center;
	color: #801da1;
}
header .resetLink{
	position: absolute;
    top: 0;
    left: 0;
    width: 1.15rem;
    height: 1.15rem;
	line-height: 1.15rem;
	text-align:center;
	color:#666;
}
.home-page{
	position:relative;
}
.search_order{
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	width:100%;
	border-bottom:1px solid #d7d7d7;
	height:1.2rem;
}
.search_order a{
	-webkit-appearance: none;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	display:block;
	text-align:center;
	line-height:1.2rem;
}
.search_order a.active,
.search_order a.jiageDouble{
	color:#801da1;
	border-bottom:1px solid #801da1;
}
.search_order a.moren i{
	background:url(../images/searchList_icon.png);
	background-size:100%;
	width:16px;
	height:16px;
	display:inline-block;
	vertical-align:middle;
	position:relative;
	top:-.02rem;
}
.search_order a.moren.active i{
	background-position:0 -35px;
}
.search_order a.jiage{
}
.search_order a.jiage i{
	background:url(../images/searchList_icon.png) 0 -70px no-repeat;
	background-size:100%;
	width:16px;
	height:16px;
	display:inline-block;
	vertical-align:middle;
}
.search_order a.jiage i{
	background:url(../images/searchList_icon.png) 0 -70px no-repeat;
	background-size:100%;
	width:16px;
	height:16px;
	display:inline-block;
	vertical-align:middle;
}
.search_order a.jiage.active i{
	background-position:0 -105px;
}
.search_order a.jiage.jiageDouble i{
	background-position:0 -140px;
}

.search_order a.xiaoliang{
}
.search_order a.xiaoliang i{
	background:url(../images/searchList_icon.png) 0 -175px no-repeat;
	background-size:100%;
	width:16px;
	height:16px;
	display:inline-block;
	vertical-align:middle;
}
.search_order a.xiaoliang.active i{
	background-position:0 -210px;
}

.specialActivity_list{
	height:auto;
	width:100%;
	margin-top:.2rem;
}
.specialActivity_list a{
	display:block;
	width:50%;
	float:left;
	padding-top:1px;
}
.specialActivity_list a > div{
	border-bottom:1px solid #d7d7d7;
	padding:0 .32rem .24rem .32rem;
	position:relative;
}
.specialActivity_list a > div .soldOut {
    width: 1.813rem;
    height: 1.813rem;
    background-image: url(../images/ico_soldOut.png);
    background-size: 100%;
    position: absolute;
    left:1.54rem;
    top: 1.54rem;
}
.specialActivity_list a > div .jpsg{
	position:absolute;
	left:.4rem;
	top:.3rem;
	width:.78rem;
	height:.78rem;
	background:url(../images/jpsg.png) no-repeat;
	background-size:100%;
}
.specialActivity_list a:nth-child(2n) > div{
	border-left:1px solid #d7d7d7;
}
.specialActivity_list a:last-child > div,
.specialActivity_list a:nth-last-child(2) > div{
	border-bottom:0;
}
.specialActivity_list a > div > img{
	width:4.2rem;
    height:4.2rem;
}
.specialActivity_list a .country {
    display: block;
    margin: .4rem 0 .12rem;
    line-height: .54rem;
	color:#333;
	font-size:.24rem;
}
.specialActivity_list a .country i {
    width: .46rem;
    height: .46rem;
    display: inline-block;
    margin-right: .2rem;
    vertical-align: top;
}
.specialActivity_list a .country img {
    width: 100%;
    height: 100%;
    display: block;
}
.specialActivity_list a p{
	color:#333;
	margin:0 0 .2rem;
	line-height:.5rem;
	height:1rem;
	display:block;
	overflow:hidden;
}
.specialActivity_list a .nowPrice {
    float: left;
    color: #ed1946;
    font-size: .54rem;
    line-height: .6rem;
    display: block;
	font-weight:bold;
}
.specialActivity_list a .oldPrice {
    text-decoration: line-through;
    float: right;
    color: #999;
    display: inline-block;
    line-height: .6rem;
	font-size:.32rem;
}
.specialActivity_list a .nowPrice:first-letter{
	font-size:.4rem;
	font-weight:normal;
	padding-right:.06rem;
}

/* 切换筛选显示 */
.menu-slider,
.second-slider{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    background: #f7f7f7;
    z-index: 102;
    overflow-y: auto;
}
.rollIn-animate {
    transform: translate3d(0,0,0);
    -webkit-transform: translate3d(0,0,0);
    -moz-transform: translate3d(0,0,0);
    -o-transform: translate3d(0,0,0);
    -ms-transform: translate3d(0,0,0);
    width: 80%;
    right: -80%;
}
.rollIn-slideLeft {
    transform: translate3d(-100%,0,0);
    -webkit-transform: translate3d(-100%,0,0);
    -moz-transform: translate3d(-100%,0,0);
    -o-transform: translate3d(-100%,0,0);
    -ms-transform: translate3d(-100%,0,0);
}
.rollIn-animate, .rollOut-animate {
    -o-transition: all .5s ease;
    -ms-transition: all .5s ease;
    -webkit-transition: all .5s ease;
    transition: all .5s ease;
    -moz-transition: all .5s ease;
}
.rollOut-slideLeft {
    transform: translate3d(-80%,0,0);
    -webkit-transform: translate3d(-80%,0,0);
    -moz-transform: translate3d(-80%,0,0);
    -o-transform: translate3d(-80%,0,0);
    -ms-transform: translate3d(-80%,0,0);
}
.mask{
	position:absolute;
	left:0;
	top:0;
	background-color:rgba(0,0,0,.6);
	z-index:111;
	display:none;
}
.showHave{
	height:1.18rem;
	background-color:#fff;
}
.helpList{
	padding:.3rem 0 0;
}
.helpList dl{
	background-color:#fff;
	padding:.3rem .28rem;
	border-bottom:1px solid #d7d7d7;
	height:1rem;
	line-height:.4rem;
}
.showHave dl{
	padding:0 .28rem;
}
.helpList a:last-child dl{
	border-bottom:0;
}
.helpList dl:after,
.showHave dl:after{
	content: " ";
	display: block;
	visibility: hidden;
	height: 0;
	clear: both;
}
.helpList dl dt{
	float:left;
	width:80%;
}
.showHave dl dt{
	float:left;
	width:60%;
}
.showHave dl dt{
	line-height:1.18rem;
}
.helpList dl dd{
	width:.5rem;
	float:right;
	vertical-align:top;
}
.second-slider .helpList dl dd{
	color:#27c241;
}
.showHave dl dd{
	width: 1.94rem;
    float: right;
    height: 1.18rem;
    padding: 0 .32rem;
	position:relative;
	left:.32rem;
}
.showHave dl dd i{
	width:44px;
	height:23px;
	display:block;
	background:url(../images/showOff.png) 0 -1px no-repeat;
	background-size:100%;
	margin-top:.2rem;
}
.showHave dl dd.onShow i{
	width:44px;
	height:23px;
	display:block;
	background:url(../images/showOff.png) 0 -56px no-repeat;
	background-size:100%;
	margin-top:.2rem;
}