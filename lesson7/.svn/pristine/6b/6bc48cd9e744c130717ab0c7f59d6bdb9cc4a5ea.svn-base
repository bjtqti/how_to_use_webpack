@charset "utf-8";
/* CSS Document */

body{
	background-color:#f7f7f7;
	padding-bottom:.32rem;
}

.listMain {
    width: 100%;
    overflow-x: hidden;
}
.listMain .tabCon {
}
.listMain .tabCon > div {
    float: left;
}

/* tabs */
.polyTabs {
	height: 1.16rem;
	border-bottom: 1px solid #d7d7d7;
	background-color:#fff;
	position:relative;
	z-index:2;
}
.polyTabs ul {
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	width: 100%;
	height: 1.16rem;
}
.polyTabs ul li {
	-webkit-appearance: none;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	display: block;
	line-height: 1.16rem;
	text-align: center;
}
.polyTabs ul li.current {
	border-bottom:.053333rem solid #801da1;
}
.polyTabs ul li.current i{
	color: #801da1;
    
}

/* all */
.listMain .tabCon > div {
	width:100%;
	position:absolute;
}
.listMain .order-box {
    border-top:.266667rem solid #f7f7f7;
	background-color:#fff;
}
.order-up, .order-down {
    padding:.24rem .32rem;
}
.order-up{
	border-bottom: 1px solid #d7d7d7;
}
.order-up span{
	color:#666;
	font-size:.27rem;
	display:inline-block;
	line-height:.48rem;
}
.order-up span em{
	padding-left:.3rem;
}
.order-up .right{
	font-size:.27rem;
	line-height:.56rem;
	text-align:right;
}
.order-up .right i{
	color:#801da1;
	margin-right:.24rem;
	display:inline-block;
	/*float:right;*/
}
.order-up .right em {
    color: #ed1946;
    /*float: right;*/
	display:inline-block;
}
.comment .order-up .right em,
.complete .order-up .right em{
	color:#27c241;
}
.order-list {
    border-bottom: 1px solid #d7d7d7;
    padding-bottom: 0;
}
.J_moveRight {
    position: relative;
    padding:0 .16rem;
}
.J_moveRight > div {
    padding:.32rem 0;
}
.J_moveRight .img_wrap {
    width:2.08rem;
    height:2.08rem;
    padding:2px;
    border: 1px solid #d7d7d7;
    background-color: #fff;
    float: left;
    font-size: 0;
    overflow: hidden;
	margin:0 .16rem;
}
.J_moveRight .img_wrap img{
	width:100%;
	height:100%;
}
.J_moveRight .gd_info {
    position: relative;
    /*width:70%;*/
    overflow: hidden;
}
.J_moveRight .gd_info .name {
    width: 100%;
    line-height: .54rem;
    height: 1.08rem;
    margin:0 0 .4rem;
    font-size:.32rem;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
}
.J_moveRight .gd_info .value{
	color:#666;
	font-size:.32rem;
	font-weight:bold;
	display:block;
}
.J_moveRight .gd_info .value:first-letter{
	font-size:.266667rem;
	padding-right:.04rem;
}
.J_moveRight .gd_info .value span {
    font-size: 16px;
    color: #333;
    z-index: 98;
}
.order-down span em {
    color: #ed1946;
    font-size: .38rem;
	font-weight:bold;
	line-height:.8rem;
	display:inline-block;
}
.order-down span em:first-letter{
	font-size:.266667rem;
	padding-right:.06rem;
}
.order-down a {
    float: right;
    padding:0 .36rem;
    border: 1px solid #ed1946;
	background-color:#ed1946;
    color: #fff;
    position: relative;
    word-spacing: 8px;
    letter-spacing: 1px;
    border-radius: 5px;
	height:.8rem;
	line-height:.8rem;
}
.order-down a.view_c{
	background-color:#fff;
	color:#ed1946;
	margin-right:.18rem;
}