@charset "utf-8";
/* CSS Document */

@-webkit-keyframes fadeIn{
	0%{
		opacity:0
	}100%{
		opacity:1
	}
}
.fadeIn{
	-webkit-animation:fadeIn 1s;
}
@-webkit-keyframes shake{
	0%,50%,100% {
		-webkit-transform:translate3d(0,0,0);
	}25%  {
		-webkit-transform:translate3d(6px,0,0);
	}75%  {
		-webkit-transform:translate3d(3px,0,0);
	}
}
.shake{
	-webkit-animation:shake 0.5s;
}

header{
	position:relative;
	z-index:2;
}

/* 类别 */
.polyTabs {
	height: 1.16rem;
	border-bottom: 1px solid #d7d7d7;
	background-color:#fff;
	position:relative;
	z-index:2;
}
.polyTabs ul {
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	width: 100%;
	height: 1.16rem;
}
.polyTabs ul li {
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
	display: block;
	line-height: 1.16rem;
	text-align: center;
}
.polyTabs ul li.current {
}
.polyTabs ul li.current i{
	color: #801da1;
    border-bottom: .053333rem solid #801da1;
}
.polyCon{
	width: 100%;
	overflow-x: hidden;
}
.polyCon #page-content{
}
.polyCon #page-content > div{
	position:absolute;
	width:100%;
	padding-bottom:1.38rem;
}
.category-bd {
    width:25%;
    height: 100%;
    position:fixed;
    z-index: 0;
    top: 0;
    left: 0;
    background-color: #f7f7f7;
}

.leftNav{
	position:relative;
	z-index:1;
	width:25%;
	float:left;
}
.leftNav .name{
    width:100%;
    line-height:1.34rem;
    height:1.34rem;
    text-align: center;
    background-color: #f3f4f6;
	border-bottom:1px solid #d7d7d7;
}




.leftNav .name .bd-on {
    /*display: none;*/
	
}
.leftNav .name.on .bd-on {
    border-left: 5px solid #801da1;
    display: inherit;
	background-color:#fff;
	-webkit-animation:shake 0.5s;
}
.leftNav .name.on .bd {
	display:none;
}
.leftNav .name .bd > div {
    font-size: .32rem;
    color: #333;
}
.rightCon{
	width:75%;
	float:right;
	position:relative;
	z-index:1;
	background-color:#fff;
}
.rightCon > div{
	display:none;
	padding:.32rem;
}
.rightCon > div.sh {
    display: inherit;
}
.rightCon .sh a.ad{
}
.rightCon a.ad img{
	width:100%;
}
.rightCon .content {

}
.rightCon .content .title {
    line-height:1.34rem;
    height:1.34rem;
    font-size:.27rem;
    padding-left: .48rem;
    background: url(../images/hotKind.png) no-repeat left;
    background-size: auto 25%;
}
.rightCon .content .cg {
    width: 32%;
    text-align: center;
    display: inline-block;
    margin-bottom: .6rem;
}
.rightCon .content .cg img {
    width: 80%;
    margin: 0 10%;
}
.rightCon .content .cg div{
	font-size:.27rem;
}

/* 品牌 */
.allBrand{
	text-align:center;
	height:1.2rem;
	line-height:1.2rem;
	display:block;
	border-bottom:1px solid #d7d7d7;
	color:#801da1;
	font-size:.373333rem;
}
.allBrand .iconfont{
	font-size:.36rem;
	position:relative;
	top:-.02rem;
	margin-left:.06rem;
	color:#801da1;
}
.poly_2 .title{
	margin:.32rem;
}
.brandList{
	margin:.32rem .16rem;
}
.brandList a{
	width:33%;
	float:left;
}
.brandList a div{
	margin:.16rem;
}
.brandList a div img{
	display:block;
	width:100%;
	border:1px solid #d7d7d7;
}

/* 产地 */
.kindArea{
	margin:.32rem;
}
.kindArea a{
	margin-bottom:.32rem;
	display:block;
}
.kindArea a img{
	width:100%;
}

/* 全部品牌 */
.brand_all{
	width:100%;
	z-index:991;
	background-color:#f7f7f7;
	overflow:auto;
}
.tt{
	position:fixed;
	top:2rem;
	right:0;
	z-index:992;
}
.tt .letterLi{
	font-size:.24rem;
	padding:0 .2rem;
	height:.49rem;
	text-align:center;

}
.brand_all #con{
}
.brand_all .brand_all_letter{
	
}
.brand_all .brand_all_letter span{
	display:block;
	height:.54rem;
	line-height:.54rem;
	background-color:#e5e5e5;
	padding-left:.32rem;
	font-size:.32rem;
}
.brand_all .brand_all_letter a{
	display:block;
	height:1.2rem;
	line-height:1.2rem;
	border-bottom:1px solid #d7d7d7;
	padding-left:.32rem;
}